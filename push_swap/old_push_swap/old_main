void	print(int stack_a[], int stack_b[], int len)
{
	int	x;

	x = 0;
	return ;
	printf("\nA:");
	while (x < len)
	{
		printf("%d ", stack_a[x]);
		x++;
	}
	x = 0;
	printf("\nB:");
	while (x < len)
	{
		printf("%d ", stack_b[x]);
		x++;
	}
	printf("\n");
}

int rotate_check_a(int stack[], int p_val, int count, int high)
{
	int i;

	i = 0;
	while (i <= high - count)
	{
		if (stack[i] > p_val)
			i++;
		else
			return (1);
	}
	return (0);
}

